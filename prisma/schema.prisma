generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url = env("DATABASE_URL")
}

model Donation {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  address     String?
  available   Boolean @default(true)

  donorId     String
  recipientId String? 
}
